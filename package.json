    "react-router-redux": "^4.0.4",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.2.5",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2"
